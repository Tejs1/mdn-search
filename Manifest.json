{
  "manifest_version": 2,
  "name": "MDN Search",
  "description": "Add MDN Search in context menu extension!",
  "version": "1.0",
  "icons": {
    "128": "mdn128.png",
    "48": "mdn48.png",
    "16": "mdn16.png"
  },
  "browser_action": {
    "default_icon": "mdn16.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["eventPage.js"],
    "persistent": false
  },
  "permissions": ["storage", "notifications", "contextMenus"],
  "options_page": "options.html"
}
